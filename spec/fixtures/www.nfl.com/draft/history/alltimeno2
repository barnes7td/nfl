<script type="text/javascript">
var loggingOff = true;
var noOmnitureCall = false;
//applyFireBugX();
isPageTypeError();
//moved all the functions into s_code_common.js until we can overhaul this atrocity

/**
* s_analytics.trackLinkClick will send a request to omniture indicating a minor level page event occurred
* @param {String} _propNum is a number provided by Marketing.  ex, prop1, prop14, prop35, etc.
* @param {String} _propValue is the value to place within the prop.  ex, Avatar, Friends_alerts, Message_Alerts, Global_Alerts, etc.
* TO DO: update this function so it accepts multiple prop values.  
*/
s_analytics.trackLinkClick = function(_propNum, _propValue) {
	var s_analytics = s_gi(s_account),
	    propName    = 'prop' + _propNum;
	s_analytics.linkTrackEvents = "";
	s_analytics.linkTrackVars = "prop" + _propNum;
	s_analytics[propName] = _propValue;
	s_analytics.events = '';
	s_analytics.tl(true, 'o', _propValue);
	// should empty out the track properties after the call is made.  this is a directive from omniture.
	s_analytics.linkTrackEvents = "";
	s_analytics.linkTrackVars = "";
}

/* analytics_file: blank.js */
function initAnalytics() { 
	isLog('hello from initAnalytics() written in blank.js');
	// the below code was added to ensure we didn't forget to tag any pages
	var section = 'nfl';
	var pathname = window.location.pathname;
	var channel = pathname.split('/')[1];
	var pageName = '';
	var heir = '';
	var queryString = window.location.search.substring(1);
	var sortQueryStringParams = function(str) {
	    var sortedStr = '';
		var strArray = str.split("&");
		if (strArray.length > 1) {
			strArray.sort();
		}
		for (var i = 0; i < strArray.length; i++) {
			var pos = strArray[i].indexOf('=');
			if (pos == -1) continue;
			var argname = strArray[i].substring(0, pos);
			if (argname == "campaign" || argname == "icampaign") continue;
			sortedStr += strArray[i];
		}
		return sortedStr;
	}
	var formatPathName = function(str) {
		str = str.replace(/\..+$/, '');
		str = str.replace(/\//, '');
		if (str.length == 0) str = 'landing';
		return formatStr(str);
	}
	var formatPageName = function(section, str, query) {
		str = section + ":" + str.replace(/\//g, ':');
		if (str.indexOf(':') == -1) {str = str + ':landing';} 
		if (query.length > 0) {
			str += ":" + query.replace(/\&/g, ':');
		}
		return formatStr(str);
	}
	var formatHierarchy = function(section, str, query) {
		str = section + ":" + str.replace(/\//g, '|');
		if (str.indexOf('|') == -1) {str = str + '|landing';}  
		if (query.length > 0) {
			str += "|" + query.replace(/\&/g, '|');
		}
		return formatStr(str);
	}
	var setOmnitureTags = function(ch, pn, hr) {
		s_analytics.server='nfl.com';	
		s_analytics.prop22='www.nfl.com'; 
		s_analytics.channel = ch; 
		s_analytics.eVar3 = ch;
		s_analytics.pageName = pn; 
		s_analytics.eVar2 = pn;
		s_analytics.hier1 = hr;  
		s_analytics.prop5 = hr;
		s_analytics.eVar4 = hr;
	}	
	var formatStr = function(str) {
		str = str.replace(/;jsessionid=(.+)/, ""); //strip out jsessionid var that gets added by server at random times
		return str;
	}
	pathname = formatPathName(pathname);
	queryString = sortQueryStringParams(queryString);
	pageName = formatPageName(section, pathname, queryString);
	hier = formatHierarchy(section, pathname, queryString);
	setOmnitureTags(channel, pageName, hier);
} 
function applyFooter() {  
	if (noOmnitureCall == true) return;
	/******* ****** DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
	try { 
		var s_code = s_analytics.t();
		if (s_code) document.write(s_code);
	} catch(e) { 
		console.warn(e);
	}
}
</script>
