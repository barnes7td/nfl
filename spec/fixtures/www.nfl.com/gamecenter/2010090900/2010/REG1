<script type="text/javascript">
	//create new slideshow nonesense
	function removePhotosOnZero(){
		if(!document.loaded){
			document.observe('dom:loaded',removePhotosOnZero);
			return
		}
		if($('gc-photos')){
			$('gc-photos').remove();
			console.log('removePhotosOnZero: removed photos node');
		}else{
			try{setTimout(removePhotosOnZero,1000)}catch(e){}
		}
	}
	if(66 > 0){
		document.observe('photos:carousel:start',function(event){
			var elapse	= 10;
			var indicatorWidth	= ($('gc-photos-indicator').getWidth());
			try{gcPhotosCarousel.timer.stop();}catch(e){};
			try{gcPhotosCarousel.indicatorMorph.cancel();}catch(e){};
			
			gcPhotosCarousel.indicatorMorph	= new Effect.Morph('gc-photos-indicator', {
				style: 'left: 0px;',
				duration: (elapse - 1),
				beforeStart: function(){$('gc-photos-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});}
			});
			gcPhotosCarousel.timer	= new PeriodicalExecuter(function(){
				document.fire('photos:carousel:scroll',{direction:'right',id:'gc-photos-carousel'});
				try{ gcPhotosCarousel.indicatorMorph.cancel(); }catch(e){};
				$('gc-photos-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});
				gcPhotosCarousel.indicatorMorph	= new Effect.Morph('gc-photos-indicator', {
					style: 'left: 0px;',
					duration: (elapse - 1),
					beforeStart: function(){$('gc-photos-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});}
				});
			},elapse);
			if($('gc-photos-controls')){
				$('gc-photos-controls').addClassName('playing');
			}
		});
		document.observe('photos:carousel:stop',function(event){
			try{gcPhotosCarousel.timer.stop();}catch(e){}
			gcPhotosCarousel.timer	= null;
			try{ gcPhotosCarousel.indicatorMorph.cancel(); }catch(e){};
			var indicatorWidth	= ($('gc-photos-indicator').getWidth());
			
			if($('gc-photos-controls')){
				$('gc-photos-controls').removeClassName('playing');
			}
			$('gc-photos-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});
		});
		var photosWidgetDestroy	= function(){
			try{ gcPhotosCarousel.timer.stop();}catch(e){};
			try{ gcPhotosCarousel.timer	= null;}catch(e){};
			try{ gcPhotosCarousel.indicatorMorph.cancel(); }catch(e){};
			try{ gcPhotosCarousel.indicatorMorph = null;}catch(e){};
			try{ gcPhotosCarousel.destroy();}catch(e){};
			console.info('destroyed photos widget');
			document.stopObserving('tab:content:destroy',photosWidgetDestroy);
		}
		document.observe('tab:content:destroy',photosWidgetDestroy);
		
		var opts	= {'rows':1,'debug':false,'evtprefix':'photos'};
		var gcPhotosCarousel	= new nfl.widgets.Carousel('gc-photos-carousel',opts);
		gcPhotosCarousel.timer			= null;
		gcPhotosCarousel.indicatorMorph	= null;
		
		document.fire('photos:carousel:start');
	}else{
		/* hide photos widget all together */
		removePhotosOnZero();
	}
</script>
	</div>
