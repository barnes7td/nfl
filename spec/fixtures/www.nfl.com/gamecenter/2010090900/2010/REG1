						<script type="text/javascript">
						(function(){
							document.write('<div class="adcontainer" id="'+nfl.global.gc.ads.rr.adObject.oid+'" style="width:160px;height:600px;"><iframe width="160" scrolling="no" height="600" frameborder="0" name="'+nfl.global.gc.ads.rr.adObject.oid+'-iframe" id="'+nfl.global.gc.ads.rr.adObject.oid+'-iframe"></iframe></div>');
							//nfl.global.gc.ads.rr.adObject.params		= (nfl.global.gc.ads.rr['160x600']);
							nfl.global.gc.ads.rr.adObject.rotate 		= true;
							nfl.global.gc.ads.rr.adObject.adClassInt	= (nfl.ads.visibleAds + 1);
							nfl.ads.rotation.interval 					= 60;
							if(nfl.ads.hasRotating === false) {
								nfl.ads.hasRotating	= true;
								if (document.loaded){ nfl.ads.rotation.start(); }else{ document.observe("dom:loaded", function () { nfl.ads.rotation.start(); });}
							}
						})()
						</script>
						<p id="ad-above-300" class="ad-above"><img src="http://img.static.nfl.com/static/site/3.5/img/ads/ad-above.gif" alt="The previous image was an ad." height="9" width="300" /></p>
						<p id="ad-above-160" class="ad-above"><img src="http://img.static.nfl.com/static/site/3.5/img/ads/ad-above-160.gif" alt="The previous image was an ad." height="9" width="160" /></p>	
					</div>
					<div id="gc-rr-watch-content">
						
<div id="channel-info" class="secondary-column-widget" style="position:absolute;visibility:hidden;overflow:hidden">
	<h3 class="heading">
		<img src="http://static.nfl.com/static/content/public/image/videos/channels/nfl-hq-videos/unlock-higher-quality.png" alt="Unlock Higher Quality" />
	</h3>
	<div style="color:#333; line-height:16px; padding:6px 8px 12px">
		<p>Improve the NFL.com viewing experience. Get free access to higher quality video by installing Akamai NetSession.
			<br />
			<a href="http://www.akamai.com/html/misc/akamai_client/netsession_interface_faq.html" target="_blank">FAQ</a> |
			<a href="http://www.akamai.com/html/misc/akamai_client/netsession_interface_tech_info.html" target="_blank">Technical Info</a>
		</p>
	</div>
	<div style="text-align:center;">
		<p style="height:50px;margin-bottom:9px" class="download-button"><a class="button-3" href="#"><span>Download &gt;</span></a></p>
		<p style="margin-bottom:7px"><img src="http://static.nfl.com/static/content/public/image/videos/channels/nfl-hq-videos/powered-by-akamai.png" alt="Delivered By Akamai" /></p>
	</div>
</div>
<script type="text/javascript">
nfl.use('akamai-dlm', 'node', 'transition', 'netsession-installer', function (Y) {
	var parent = Y.one("#channel-info"), h;
	if (DLMHelper.installed) {
		parent.remove();
	}
	else {
		Y.on('load', function () {
			h = parent.getStyle('height');
			parent.setStyles({ opacity: 0, height: 0, position: 'static', visibility: 'visible' }).transition({ height: h, duration: 0.25 }, function () {
				this.transition({ opacity: 1, duration: 0.25 });
			});
			parent.one(".download-button a").on('click', function (event) {
				event.halt();
				Y.NetSessionInstaller.installNetSession("GC_Watch_HQ_DL");
			});
			Y.Global.on(Y.NetSessionInstaller.INSTALL_COMPLETE, function (event) {
				parent.transition({ opacity: 0, duration: 0.25 }, function () {
					this.transition({ height: 0, duration: 0.25 }, this.remove);
				});
			});
		});
	}
});
</script>
						
						
