		try{ gcShopCarousel.timer.stop();}catch(e){};
		try{ gcShopCarousel.timer	= null;}catch(e){};
		try{ gcShopCarousel.indicatorMorph.cancel(); }catch(e){};
		try{ gcShopCarousel.indicatorMorph = null;}catch(e){};
		try{ gcShopCarousel.destroy();}catch(e){};
		console.info('destroyed shop widget');
		document.stopObserving('tab:content:destroy',shopWidgetDestroy);
	}
	document.observe('tab:content:destroy',shopWidgetDestroy);
	var opts	= {'rows':1,'debug':false,'evtprefix':'nfl'};
	var gcShopCarousel	= new nfl.widgets.Carousel('gc-shop-carousel',opts);
	gcShopCarousel.timer			= null;
