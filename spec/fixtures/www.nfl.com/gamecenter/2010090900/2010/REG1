			style: 'left: 0px;',
			duration: (elapse - 1),
			beforeStart: function(){$('gc-shop-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});}
		});
		gcShopCarousel.timer	= new PeriodicalExecuter(function(){
			document.fire('nfl:carousel:scroll',{direction:'right',id:'gc-shop-carousel'});
			try{ gcShopCarousel.indicatorMorph.cancel(); }catch(e){};
			$('gc-shop-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});
			gcShopCarousel.indicatorMorph	= new Effect.Morph('gc-shop-indicator', {
				style: 'left: 0px;',
				duration: (elapse - 1),
				beforeStart: function(){$('gc-shop-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});}
			});
		},elapse);
		if($('gc-shop-controls')){
			$('gc-shop-controls').addClassName('playing');
		}
	});
	document.observe('nfl:carousel:stop',function(event){
		try{gcShopCarousel.timer.stop();}catch(e){}
		gcShopCarousel.timer	= null;
		try{ gcShopCarousel.indicatorMorph.cancel(); }catch(e){};
		var indicatorWidth	= ($('gc-shop-indicator').getWidth());
		
		if($('gc-shop-controls')){
			$('gc-shop-controls').removeClassName('playing');
		}
		$('gc-shop-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});
	});
