	nfl.global.gc.extdata.media		= {"tv":"NBC","sat":retDefinedValue(),"sathd":retDefinedValue(),"radio":{"home":retDefinedValue(126),"away":retDefinedValue(125)}};
	nfl.global.gc.extdata.config	= {"gameState":"POST","config":{"epicFail":false,"adinterval":60,"videoadinterval":-1,"cookieExpiry":0,"adevents":["onload","endofthird","pregame"]}}
	
	nfl.global.gc.ads.rr			= {};
	nfl.global.gc.ads.dc			= {};
	nfl.global.gc.ads.vp			= {};
	nfl.global.gc.ads.page			= {};
	nfl.global.gc.tabs.defaultIndex	= 0;
	nfl.global.gc.tabs.defaultName	= "recap";
	//fn:toLowerCase(fn:replace(tab.tabId,'_','-'))

	nfl.global.gc.dc.load			= true;
	nfl.global.gc.dc.view			= "TOP";
	nfl.global.gc.dc.testandtarget	=null;
	
	if(typeof nfl.global.gc.extdata.config.config !== 'undefined'){ nfl.global.gc.extdata.config = nfl.global.gc.extdata.config.config; /* fix it*/ }
	if(nfl.global.gc.game.state == 'PRE' || nfl.global.gc.game.state == 'ACTIVE'){
		/* I know this looks just like pregame data but it also serves a purpose for active games that do not yet have gtds as well */
