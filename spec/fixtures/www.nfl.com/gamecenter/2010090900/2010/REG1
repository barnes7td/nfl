	nfl.global.gc.ads.pathMutator			= function(config){
		if(config.type === 'rr'){
			var adPath = "nfl.gamecenter/gamecenter/#{tab.id}/2010/REG1;s1=gamecenter;s2=#{tab.id};s3=2010;s4=REG1;slot=rightrailfirst;url=gamecenter_#{tab.id}_2010_REG1;nfl=ad;!category=;kw=;team=min;team=no;mnf=thursday;gameid=2010090900;tile="+ config.tile +";test=;sz="+ config.size +"";
			return adPath
		}
		if(config.type === 'event'){
			var adPath = "nfl.gamecenter/gamecenter/tab_gamecenter/2010/REG1;s1=gamecenter;s2=tab_gamecenter;s3=2010;s4=REG1;slot=inpage;break="+config.eventname+";url=gamecenter_tab_gamecenter_2010_REG1;nfl=ad;!category=;kw=;team=min;team=no;mnf=thursday;gameid=2010090900;tile=10;test=;sz=640x360;ord=";
			return adPath
		}
		if(config.type === 'background'){
			return 'http://ad.doubleclick.net/adj/'+config.path;
		}
		if(config.type === 'video'){
			return adPath	= "http://ad.doubleclick.net/pfadx/nfl.gamecenter/gamecenter/tab_watch/2010/reg2;s1=gamecenter;s2=tab_watch;s3=2010;s4=REG1;slot=inpage;url=gamecenter_tab_gamecenter_2010_REG1;nfl=ad;!category=;kw=;team=min;team=no;mnf=thursday;gameid=2010090900;tile=10;test=;sz=640x360;ord=" + adRandom + "?"; 
		}
	}
	nfl.global.gc.ads.dc.showOnLoad = function(cookieExp){
		console.info('showOnLoadAd',cookieExp);
		if(cookieExp > -2){
			/* show ad */
			document.fire('gamecenter:data:adevent',['onload']);
			if(cookieExp === 0){ return; }
			if(cookieExp === -1){ document.cookies.set('gcolbia','true',false,'/'); return; }
			document.cookies.set('gcolbia','true',document.cookies.getExipiry(0,cookieExp,0),'/');
		}
	}
	nfl.global.gc.ads.page.loadBackground = function(path){
		var head = $$('head')[0], script;
		if (head)
		{
		    script = new Element('script', {type: 'text/javascript', src: path});
		    head.appendChild(script);
		}
	}
	nfl.global.gc.ads.page.renderBackground = function(e){
		var options	= e.memo;
		console.info('nfl.global.gc.ads.page.renderBackground',e);
		try{
			options.background_color = options.background_color || "#ffffff";
			options.image_url = options.image_url || "";
			options.background_tiling = options.background_tiling || "";
			options.link_url = options.link_url || "";
	
			var oBODY = document.body,
			cssBrowserPanelBG = options.background_color + " url(" + options.image_url + ") center 122px " + options.background_tiling + " fixed",
			cssDefaultBG = 'url(/img/global/alt/body.jpg) no-repeat scroll center 102px #FFFFFF';
	
			if (document.cookies.get('gcolbia') == '' || document.cookies.get('gcolbia') == 'true')
			{
				oBODY.style.background = cssBrowserPanelBG;
			}
			
			document.observe(nfl.events.VideoPlayerEvent.AD_LOADED,function(event){
			    if (event.memo.swfid === 'drive_chart_ads') {
					oBODY.style.background = cssDefaultBG;
			    }
			});
	
			document.observe(nfl.events.VideoPlayerEvent.AD_COMPLETE,function(event){
			    if (event.memo.swfid === 'drive_chart_ads') {
					oBODY.style.background = cssBrowserPanelBG;
			    }
			});
		}catch(e){ console.error('nfl.global.gc.ads.page.renderBackground',e); }
	}
	// BG
	
		var adRandom = (nfl.constants.AD_RANDOM)? nfl.constants.AD_RANDOM : adRandom ;
		var adPath = nfl.ads.createAd("ad1312841864136background", 
			"nfl.gamecenter/gamecenter;s1=gamecenter;slot=background;url=gamecenter;nfl=ad;!category=;kw=;team=min;team=no;mnf=thursday;gameid=2010090900;tile=#{tile};test=;sz=1x1", 
			{
				width: 1,
				height: 1,
				slot: 'background',
				transparent: false,
				pathOnly: true
			},
			Y
		);
	
	nfl.global.gc.ads.page['background']	= nfl.global.gc.ads.pathMutator({type:'background',path:adPath});
	// HEADER
	
		var adRandom = (nfl.constants.AD_RANDOM)? nfl.constants.AD_RANDOM : adRandom ;
		var adPath = nfl.ads.createAd("ad1312841864136spon", 
			"nfl.gamecenter/gamecenter;s1=gamecenter;slot=spon;url=gamecenter;nfl=ad;!category=;kw=;team=min;team=no;mnf=thursday;gameid=2010090900;tile=#{tile};test=;sz=985x55", 
			{
				width: 985,
				height: 55,
				slot: 'spon',
				transparent: true,
				pathOnly: true
			},
			Y
		);
	
	nfl.global.gc.ads.page['header'] 		= adPath;
	// RR (calls one real ad tag to increment ads collection, reuses new ad's tile)
	
		var adRandom = (nfl.constants.AD_RANDOM)? nfl.constants.AD_RANDOM : adRandom ;
		var adPath = nfl.ads.createAd("ad1312841864137rightrailfirst", 
			"nfl.gamecenter/gamecenter;s1=gamecenter;slot=rightrailfirst;url=gamecenter;nfl=ad;!category=;kw=;team=min;team=no;mnf=thursday;gameid=2010090900;tile=#{tile};test=;sz=160x600", 
			{
				width: 160,
				height: 600,
				slot: 'rightrailfirst',
				transparent: false,
				pathOnly: true
			},
			Y
		);
	
	nfl.global.gc.ads.rr.adObject			= nfl.ads.collection[(nfl.ads.collection.length -1)];
	nfl.global.gc.ads.rr['160x600']			= nfl.global.gc.ads.pathMutator({type:'rr', tile: nfl.ads.tilesOnPage(), size:'160x600'});
	nfl.global.gc.ads.rr['300x250']			= nfl.global.gc.ads.pathMutator({type:'rr', tile: nfl.ads.tilesOnPage(), size:'300x250'});
	// DC BIA and VP
	nfl.global.gc.ads.dc['onload']			= nfl.global.gc.ads.pathMutator({type:'event', eventname:'onload'});
	nfl.global.gc.ads.dc['pregame']			= nfl.global.gc.ads.pathMutator({type:'event', eventname:'pregame'});
	nfl.global.gc.ads.dc['endoffirst']		= nfl.global.gc.ads.pathMutator({type:'event', eventname:'endoffirst'});
	nfl.global.gc.ads.dc['halftime']		= nfl.global.gc.ads.pathMutator({type:'event', eventname:'halftime'});
	nfl.global.gc.ads.dc['endofthird']		= nfl.global.gc.ads.pathMutator({type:'event', eventname:'endofthird'});
	nfl.global.gc.ads.dc['endofgame']		= nfl.global.gc.ads.pathMutator({type:'event', eventname:'endofgame'});
	nfl.global.gc.ads.dc['timeouts']		= nfl.global.gc.ads.pathMutator({type:'event', eventname:'timeouts'});
	nfl.global.gc.ads.vp['ad640x360']		= nfl.global.gc.ads.pathMutator({type:'video'});
	
	
	if(document.cookies.get('gcolbia') == ''){
		/* do initial onload bia code */
		document.observe('gamecenter:drivechart:initialized',function(){ nfl.global.gc.ads.dc.showOnLoad.delay(2,0); });
	}
	// setup things that can only load after the head of document is rendered
	dartWallpaperRender	= function(options){ document.fire('adbackground:loaded',options);} //event proxy from dartWallpaperRender to proper event model
	document.observe('head:loaded',function(e){
		// load background ad
		document.observe('adbackground:loaded',nfl.global.gc.ads.page.renderBackground);
		nfl.global.gc.ads.page.loadBackground(nfl.global.gc.ads.page['background']); //when loaded, dart calls "dartWallpaperRender" which then fires off an event of "adbackground:loaded"
		
		// overwrite team colors if values are too close
		nfl.GameCenter.Utility.overwriteTeamColorClasses('NO','MIN');
	});
	
	</script>
	<script src="http://scripts.static.nfl.com/static/site/3.5/scripts/gc/gamecenter.init.js" type="text/javascript" ></script>
