	<script type="text/javascript">
		
(function(){
	var gameId = "2010090900",
		key = 'GC54863',
		headline = "NFL Game Center: Minnesota Vikings at New Orleans Saints - 2010 Week 1",
		url = "";

    if (nfl.constants.COMMENTS) {

		Y.use('commentmodule', 'nfl-user', function (Y) {
			var cm, noCommentsHandler;

			// set up comments module
			cm = new Y.SiteLife.CommentModule({
				list: "#gc-discuss-comments-list",
				key: key,
				headline: headline,
				url: url
			});
			noCommentsHandler = cm.get('commentList').on(Y.SiteLife.CommentList.CONTENT_UPDATE, function (event) {
				Y.one("#gc-discuss").one('div.no-comments')[event.count ? 'hide' : 'show']();
			});
			cm.render();
			
			function cleanup(_e){
				noCommentsHandler.detach();
				cm.destroy();
				//document.stopObserving('tab:content:destroy', cleanup);
			}
			
			Y.Global.on('tab:content:destroy', cleanup);
		});
	}
	else {
		YUI().use('node',function(Y){
			Y.one("#gc-discuss").one('div.comments-disabled').show();
		});
	}
})();
	</script>
</div>
<!-- END WIDGET teams/discuss -->

