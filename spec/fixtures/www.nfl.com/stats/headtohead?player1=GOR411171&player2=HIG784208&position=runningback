<script type="text/javascript">

// Inserts alternating row colors
(function() {
	function altTableRows(){
		var tables = $$('table.headToHeadTable');
		tables.each(alternateRowClasses);
	}
	function alternateRowClasses(table) {
	    var rows = table.getElementsByTagName("tr");
	    var rlength = rows.length;
	    for (i = 1; i < rlength; i++) {
		    rows[i].className = ((i%2) == 1) ? 'darkerRow' : 'whiteBack';
	    }
	}
	nfl.events.observe("domLoaded", altTableRows);
})();	

function submitForm(){
   var player1 = document.getElementById('playerOnePlayerSelect').options[document.getElementById('playerOnePlayerSelect').selectedIndex].text;
   document.getElementById('playerOne').value = player1;
   var player2 = document.getElementById('playerTwoPlayerSelect').options[document.getElementById('playerTwoPlayerSelect').selectedIndex].text;
   document.getElementById('playerTwo').value = player2;
   var player3= document.getElementById('playerThreePlayerSelect').options[document.getElementById('playerThreePlayerSelect').selectedIndex].text;
   document.getElementById('playerThree').value = player3;
   var player4 = document.getElementById('playerFourPlayerSelect').options[document.getElementById('playerFourPlayerSelect').selectedIndex].text;
   document.getElementById('playerFour').value = player4;
   if(player1 != 'Select a Player...' || player2 != 'Select a Player...' || player3 != 'Select a Player...' || player4 != 'Select a Player...'){
   	document.forms['playerHeadToHeadForm'].submit();
   }
   else{
   	$('noPlayersSelected').show();
   }
}
function showSilhouette(badImage) { 
	badImage.src = 'http://img.static.nfl.com/static/site/3.5/img/sr_pic0.gif'; 
}

function submitQuery(){ }

function removeAllChildren(element) {
	while (element.firstChild) {
		element.removeChild(element.firstChild);
	}
}
	
function setOptions(element, evalString) {
	removeAllChildren(element);
	var opts = eval(evalString);
	
	for (var i in opts){
	    var o = document.createElement('option');
		o.innerHTML = "";
		o.text = opts[i];
		o.value = i;
		element.appendChild(o);	
	} 
}

function updatePlayerDropDownList(playerNumber) {
  if(playerNumber ==1){
 	new Ajax.Request('/ajax/headtohead', { 
		 method: 'get', 
		 parameters: { seasonId: $("currentSeason").value, 
			teamId: $("playerOneTeamSelect").value,
			positionId: $("position").value }, 
		 onSuccess: function(transport) {
			setOptions($('playerOnePlayerSelect'), '({' + transport.responseText + '})');	
  		 }
	 });
  }
  if(playerNumber ==2){
 	new Ajax.Request('/ajax/headtohead', { 
		 method: 'get', 
		 parameters: { seasonId: $("currentSeason").value, 
			teamId: $("playerTwoTeamSelect").value,
			positionId: $("position").value }, 
		 onSuccess: function(transport) {
			setOptions($('playerTwoPlayerSelect'), '({' + transport.responseText + '})');
  		 }
	});
  }
  if(playerNumber ==3){
 	new Ajax.Request('/ajax/headtohead', { 
		 method: 'get', 
		 parameters: { seasonId: $("currentSeason").value, 
			teamId: $("playerThreeTeamSelect").value,
			positionId: $("position").value }, 
		 onSuccess: function(transport) {
			setOptions($('playerThreePlayerSelect'), '({' + transport.responseText + '})');
  		 }
	});
  }
  if(playerNumber ==4){
    new Ajax.Request('/ajax/headtohead', { 
		 method: 'get', 
		 parameters: { seasonId: $("currentSeason").value, 
			teamId: $("playerFourTeamSelect").value,
			positionId: $("position").value }, 
		 onSuccess: function(transport) {
			setOptions($('playerFourPlayerSelect'), '({' + transport.responseText + '})');
  		 }
	});
  }
}
// Adds functionality for "clear" buttons
(function() {
	function clearLoadedOptions(e) {
		var el, players, playerNames, teams;
		el = Event.element(e);
		players = Element.previous(el);
		playerNames = Element.immediateDescendants(players);
		for (var i = playerNames.length - 1; i > 0; i--) {
			Element.remove(playerNames[i]);
		}
		teams = Element.previous(players);
		teams.value='null';
		el = players = playerNames = teams = null;
	}
	function setCLOListeners(e) {
		var buttons = $$("div.playerComparisonChoice input[value=Clear]");
		buttons.each(function(button) {
			Event.observe(button, "click", clearLoadedOptions);
		});
		buttons = null;
	}
	nfl.events.observe("domLoaded", setCLOListeners);
})();
</script>
